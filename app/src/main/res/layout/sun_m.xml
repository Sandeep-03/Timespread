<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >



    <android.support.v7.widget.CardView
        android:id="@+id/card_view1"
        android:layout_gravity="center"
        android:layout_width="150dp"
        android:layout_height="160dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="30dp"
        card_view:cardCornerRadius="2dp"
        card_view:cardElevation="4dp">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical"
            android:fitsSystemWindows="true">

            <View
                android:id="@+id/view1"
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:background="@color/orange"
                />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/view1"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                android:fontFamily="exo -2 semi bold"
                android:textColor="@color/colorPrimary"
                android:text="Break Fast"
                android:textSize="15sp"
                android:layout_alignParentTop="true"/>

            <TextView
                android:id="@+id/textView5"
                android:text="7:30am-9:15am"
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12dp"
                android:fontFamily="roboto medium"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"/>

            <TextView
                android:id="@+id/textView6"
                android:text="Samosa/Pyaz Kachori"
                android:layout_toRightOf="@+id/view1"
                android:layout_marginLeft="15dp"
                android:textSize="12dp"
                android:fontFamily="roboto medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView4"/>
            <TextView
                android:id="@+id/textViewJA"
                android:text="Jlebi"
                android:layout_toRightOf="@+id/view1"
                android:layout_marginLeft="15dp"
                android:textSize="12dp"
                android:fontFamily="roboto medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textViewC"/>
            <TextView
                android:id="@+id/textViewO"
                android:text="Omlette(L)"
                android:layout_toRightOf="@+id/view1"
                android:layout_marginLeft="15dp"
                android:textSize="12dp"
                android:fontFamily="roboto medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView6"/>
            <TextView
                android:id="@+id/textViewB"
                android:text="Bread with Butter/jam"
                android:layout_toRightOf="@+id/view1"
                android:layout_marginLeft="15dp"
                android:textSize="12dp"
                android:fontFamily="roboto medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textViewO"/>
            <TextView
                android:id="@+id/textViewM"
                android:text="Sprouts,Coffee/Milk"
                android:layout_toRightOf="@+id/view1"
                android:layout_marginLeft="15dp"
                android:textSize="12dp"
                android:fontFamily="roboto medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textViewB"/>
            <TextView
                android:id="@+id/textViewC"
                android:text="Cornflakes/Bournvita"
                android:layout_toRightOf="@+id/view1"
                android:layout_marginLeft="15dp"
                android:textSize="12dp"
                android:fontFamily="roboto medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textViewM"/>


        </RelativeLayout>
    </android.support.v7.widget.CardView>



